<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	<bean id="dept" class="cn.mldn.mldnspring.vo.Dept" p:deptno="10"
		p:dname="财务部" />
	<bean id="hiredateObj" class="java.util.Date" />
	<bean id="emp" class="cn.mldn.mldnspring.vo.Emp" p:empno="7369"
		p:ename="SMITH" p:age="30" p:hiredate-ref="hiredateObj"
		p:dept-ref="dept" p:salary="789.98"/>
	

</beans>
